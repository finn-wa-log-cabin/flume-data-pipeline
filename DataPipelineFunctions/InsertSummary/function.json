{
  "scriptFile": "insert_summary.py",
  "bindings": [
    {
      "name": "requestMsg",
      "type": "queueTrigger",
      "direction": "in",
      "queueName": "summary-requests",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "dataJson",
      "type": "table",
      "tableName": "DeviceTelemetry",
      "partitionKey": "{queueTrigger.deviceID}",
      "filter": "-gt {queueTrigger.startTime} -lt {queueTrigger.endTime}",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    }
  ]
}