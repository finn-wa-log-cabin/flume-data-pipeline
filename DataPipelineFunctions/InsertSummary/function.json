{
  "scriptFile": "insert_summary.py",
  "bindings": [
    {
      "name": "requestMsg",
      "type": "queueTrigger",
      "direction": "in",
      "queueName": "summary-requests",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "dataJson",
      "type": "table",
      "tableName": "DeviceTelemetry",
      "partitionKey": "{partition}",
      "filter": "RowKey gt '{startTime}' and RowKey lt '{endTime}'",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    }
  ]
}