{
  "scriptFile": "insert_telemetry_aggregate.py",
  "bindings": [
    {
      "name": "requestMsg",
      "type": "queueTrigger",
      "direction": "in",
      "queueName": "telemetry-aggregate-requests",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "data",
      "type": "table",
      "tableName": "DeviceTelemetry",
      "partitionKey": "{queueTrigger.partitionKey}",
      "filter": "-gt {queueTrigger.startDate}",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    }
  ]
}